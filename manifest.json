{
    "manifest_version": 2,
    "name": "MyDoro Timer",
    "description": "MyDoro Timer - a customizable Pomodoro Timer, adjusted to your liking!",
    "version": "1.0.0",
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "html": ["popout.html"],
        "js": ["popout.js"]
        }
    ],

    "browser_action": {
        "default_title": "MyDoro Timer",
        "default_popup": "popout.html"
    }
}